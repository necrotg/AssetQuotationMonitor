<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<targets>
		<target xsi:type="File" name="fileTarget" fileName="${basedir}\logFile.txt" layout="${longdate}|${level:uppercase=true}|${logger}|${message}"/>
	</targets>
	<rules>
		<logger name="*" minlevel="Info" writeTo="fileTarget,console" />
	</rules>
</nlog>